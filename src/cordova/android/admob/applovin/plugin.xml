<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="cocoon-plugin-ads-android-admob-applovin" version="1.0.2">
    <name>AdsAdMobappLovin</name>
    <description>Android AdMob AppLovin Adapter</description>
    <license>MPL 2.0</license>
    <keywords>cordova,ads,interstitial,monetization,ad,admob,applovin</keywords>
    <repo>https://github.com/ludei/atomic-plugins-ads</repo>
    <platform name="android">
        <preference name="APP_ID" />
        
        <source-file src="src/deps/AdMobMediationInterEvent.java" target-dir="src/com/ludei/ads/applovin" />
        <source-file src="src/deps/ApplovinAdapter.java" target-dir="src/com/ludei/ads/applovin" />

        <dependency id="cocoon-plugin-ads-android-admob" version="1.0.12" />
        <dependency id="cocoon-plugin-ads-android-applovin" version="1.0.1" />
    </platform>

</plugin>