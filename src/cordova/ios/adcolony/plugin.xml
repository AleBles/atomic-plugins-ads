<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="cocoon-plugin-ads-ios-adcolony" version="1.0.3">
    <name>AdsAdColony</name>
    <description>iOS Ads AdColony implementation</description>
    <license>MPL 2.0</license>
    <keywords>cordova,ads,banner,interstitial,monetization,ad,adcolony</keywords>
    <repo>https://github.com/ludei/atomic-plugins-ads</repo>
    <platform name="ios">
        <framework src="src/deps/external/AdColony.framework" custom="true"/>

        <framework src="StoreKit.framework"/>
        <framework src="Foundation.framework"/>
        <framework src="CoreGraphics.framework"/>
        <framework src="UIKit.framework"/>
        <framework src="Social.framework" weak="true"/>
        <framework src="libz.dylib"/>
        <framework src="MediaPlayer.framework" weak="true"/>
        <framework src="WebKit.framework" weak="true"/>
        <framework src="CoreTelephony.framework" weak="true"/>
        <framework src="SystemConfiguration.framework" weak="true"/>
        <framework src="AdSupport.framework" weak="true"/>
        <framework src="MessageUI.framework" weak="true"/>
        <framework src="EventKit.framework" weak="true"/>

    </platform>
    <dependency id="cocoon-plugin-ads-ios-common"/>
</plugin>
