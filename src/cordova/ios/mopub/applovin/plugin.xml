            <header-file src="src/AppLovinRewardedCustomEvent.h" />
            <source-file src="src/AppLovinRewardedCustomEvent.m" />
    </platform>
    <dependency id="cocoon-plugin-ads-ios-applovin" />
    <dependency id="cocoon-plugin-ads-ios-mopub" />
</plugin>